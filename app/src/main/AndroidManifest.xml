<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.barry.euclid.mobile_crypto" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:targetSdkVersion="17"
        android:debuggable="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EncryptionActivity"
            android:label="@string/title_activity_encryption" >
        </activity>
        <activity
            android:name=".PrimalityTestsActivity"
            android:label="@string/title_activity_primality_tests" >
        </activity>
        <activity
            android:name=".PRNGActivity"
            android:label="@string/title_activity_prng" >
        </activity>
    </application>

</manifest>
